{
	"geode": "4.6.2",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"resources": {
		"sprites": [
			"Sprites/**/*.png"
		],
		"files": [
			"Default/**/*.png"
		]
	},
	"settings": {
		"AprilFools": {
			"name": "Enable April Fools Mode",
			"description": "Toggles April Fools to be gettable.",
			"type": "bool",
			"default": true
		},
		"Force-April-Fools": {
			"name": "Force April Fools Mode",
			"description": "Toggles April Fools always.",
			"type": "bool",
			"default": false
		},
		"Force-Winter": {
			"name": "Force Winter Mode",
			"description": "Toggles Winter always.",
			"type": "bool",
			"default": false
		},
		"Menu-Snow": {
			"name": "Enable Snow effect",
			"description": "Toggles Snow effect if it is winter.",
			"type": "bool",
			"default": true
		},
		"WinterHats": {
			"name": "Enable Icon Hats",
			"description": "Toggles hats on icons if it is winter.",
			"type": "bool",
			"default": true
		},
		"Force-Posters": {
			"name": "Force Posters in Normal Mode",
			"description": "Toggles pride posters without Pride Mode.",
			"type": "bool",
			"default": false
		},
		"Force-Pride-mode": {
			"name": "Force Pride Mode",
			"description": "Toggles Pride always.",
			"type": "bool",
			"default": false
		},
		"Pride-Icon": {
			"name": "Enable Geode 2025 (Pride)",
			"description": "Toggles 2025 pride icon.",
			"type": "bool",
			"default": true
		},
		"Posters": {
			"name": "Enable Posters in Pride",
			"description": "Toggles pride posters.",
			"type": "bool",
			"default": true
		}
	},
	"id": "viper.themed_dash",
	"name": "Themed Dash",
	"links": {"source": "https://github.com/Cosmella-v/Geometry-timed-themes"},
	"version": "v1.0.0-beta.4",
	"dependencies" : {
		"geode.node-ids": {
            "version": ">=v1.20.1",
            "importance": "required"
        }
	},
	"developer": "Viper",
	"description": "Thought that gd was boring without timed themes, This is the mod for you! Add christmas hats, April fools events and even pride month!"
}